{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
          "eslint:recommended",
          "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "func-names": "off",
      "react/jsx-props-no-spreading": [0],
      "jsx-a11y/control-has-associated-label":[0],
      "react/prop-types": [0],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
}
